<?php

namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
* @ORM\Entity
* @ORM\Table(name="collaboration_information")
*/

class CollaborationInformation
{
  /**
  * @ORM\Column(type="integer")
  * @ORM\Id
  * @ORM\GeneratedValue(strategy="AUTO")
  */
  protected $entry_id;

  /**
  * @ORM\Column(type="integer")
  */
  protected $person_id;

  /**
  * @ORM\Column(type="string")
  */
  protected $name;

  /**
  * @ORM\Column(type="string")
  */
  protected $research_group;

  /**
  * @ORM\Column(type="simple_array")
  */
  protected $disciplinary_background;

  /**
  * @ORM\Column(type="string")
  */
  protected $work_location;

  /**
  * @ORM\Column(type="date")
  */
  protected $work_start_date;

  /**
  * @ORM\Column(type="string")
  */
  protected $working_duration_before_join;

  /**
  * @ORM\Column(type="string")
  */
  protected $academic_background;

  /**
  * @ORM\Column(type="simple_array")
  */
  protected $mother_tongue;

  /**
  * @ORM\Column(type="string")
  */
  protected $gender;

  /**
  * @ORM\Column(type="integer")
  */
  protected $age;

  /**
  * @ORM\Column(type="string")
  */
  protected $nationality;

  /**
  * @ORM\Column(type="string")
  */
  protected $collaboration_with;

  /**
  * @ORM\Column(type="string")
  */
  protected $project_name;

  /**
  * @ORM\Column(type="date")
  */
  protected $project_completion_date;

  /**
  * @ORM\Column(type="string")
  */
  protected $collaborated_before;

  /**
  * @ORM\Column(type="integer")
  */
  protected $project_working_time;

  /**
  * @ORM\Column(type="simple_array")
  */
  protected $expected_outcome;
}

 ?>
